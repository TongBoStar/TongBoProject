			.append(getQs())
			.append(getFtype())
			.append(getSourceid())
			.append(getReplystatus())
			.append(getFsrcno())
			.toHashCode();
	}
	
	public boolean equals(Object obj) {
		if(obj instanceof Nogood == false) return false;
		if(this == obj) return true;
		Nogood other = (Nogood)obj;
		return new EqualsBuilder()
			.append(getRid(),other.getRid())
			.append(getCheckid(),other.getCheckid())
			.append(getWorkno(),other.getWorkno())
			.append(getItemcode(),other.getItemcode())
			.append(getItemname(),other.getItemname())
			.append(getItemmode(),other.getItemmode())
			.append(getFbatchno(),other.getFbatchno())
			.append(getSeqno(),other.getSeqno())
			.append(getEmp(),other.getEmp())
			.append(getFqty(),other.getFqty())
			.append(getCstatus(),other.getCstatus())
			.append(getCdate(),other.getCdate())
			.append(getPbdate(),other.getPbdate())
			.append(getPedate(),other.getPedate())
			.append(getCreatedate(),other.getCreatedate())
			.append(getClosestatus(),other.getClosestatus())
			.append(getQc(),other.getQc())
			.append(getQs(),other.getQs())
			.append(getFtype(),other.getFtype())
			.append(getSourceid(),other.getSourceid())
			.append(getReplystatus(),other.getReplystatus())
			.append(getFsrcno(),other.getFsrcno())
			.isEquals();
	}

