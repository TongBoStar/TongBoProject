package com.terrence.crm.dol.query;

import java.io.Serializable;

import org.apache.commons.lang.builder.ToStringBuilder;
import org.apache.commons.lang.builder.ToStringStyle;

import com.terrence.crm.dol.base.BaseQuery;


public class ApproveMsgQuery extends BaseQuery implements Serializable {
	

	
	//columns START
	private java.lang.Long id;
	private java.lang.Integer billType;
	private java.lang.Long billId;
	private java.lang.Integer optType;
	private java.lang.String approveCotent;
	private java.lang.String approver;
	private java.util.Date approverTimeBegin;
	private java.util.Date approverTimeEnd;
	//columns END


		public void setId(java.lang.Long value) {
			this.id = value;
		}
		
		public java.lang.Long getId() {
			return this.id;
		}
		public void setBillType(java.lang.Integer value) {
			this.billType = value;
		}
		
		public java.lang.Integer getBillType() {
			return this.billType;
		}
		public void setBillId(java.lang.Long value) {
			this.billId = value;
		}
		
		public java.lang.Long getBillId() {
			return this.billId;
		}
		public void setOptType(java.lang.Integer value) {
			this.optType = value;
		}
		
		public java.lang.Integer getOptType() {
			return this.optType;
		}
		public void setApproveCotent(java.lang.String value) {
			this.approveCotent = value;
		}
		
		public java.lang.String getApproveCotent() {
			return this.approveCotent;
		}
		public void setApprover(java.lang.String value) {
			this.approver = value;
		}
		
		public java.lang.String getApprover() {
			return this.approver;
		}
	
		public void setApproverTimeBegin(java.util.Date value) {
			this.approverTimeBegin = value;
		}
		
		public java.util.Date getApproverTimeBegin() {
			return this.approverTimeBegin;
		}
		public void setApproverTimeEnd(java.util.Date value) {
			this.approverTimeEnd = value;
		}
		
		public java.util.Date getApproverTimeEnd() {
			return this.approverTimeEnd;
		}
	

	public String toString() {
		return ToStringBuilder.reflectionToString(this,ToStringStyle.MULTI_LINE_STYLE);
	}
}

