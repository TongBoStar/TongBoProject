package com.terrence.crm.dol.bean;

import java.util.List;

import javax.persistence.Entity;

import org.apache.commons.lang.builder.EqualsBuilder;
import org.apache.commons.lang.builder.HashCodeBuilder;
import org.apache.commons.lang.builder.ToStringBuilder;

import com.terrence.crm.dol.base.BaseEntity;

@Entity
public class TaskStatistic extends BaseEntity implements java.io.Serializable {
	
	
	//columns START
	private java.lang.Long statisticid;
	private java.lang.Long itemid;
	private Double jan;
	private Double feb;
	private Double mar;
	private Double apr;
	private Double may;
	private Double jun;
	private Double july;
	private Double aug;
	private Double sept;
	private Double oct;
	private Double nov;
	private Double dec;
	private Double yearly;
	private java.lang.Integer ftype;
	//columns END
	private String itemname;
	private Integer fkey;
	private List<StatisticItem> itemnameList;
	private java.lang.Long fclassid;
	private List<StatisticClass> statisticclassList;
	
	public void setStatisticid(java.lang.Long value) {
		this.statisticid = value;
	}
	
	public java.lang.Long getStatisticid() {
		return this.statisticid;
	}
	public void setItemid(java.lang.Long value) {
		this.itemid = value;
	}
	
	public java.lang.Long getItemid() {
		return this.itemid;
	}
	public void setJan(Double value) {
		this.jan = value;
	}
	
	public Double getJan() {
		return this.jan;
	}
	public void setFeb(Double value) {
		this.feb = value;
	}
	
	public Double getFeb() {
		return this.feb;
	}
	public void setMar(Double value) {
		this.mar = value;
	}
	
	public Double getMar() {
		return this.mar;
	}
	public void setApr(Double value) {
		this.apr = value;
	}
	
	public Double getApr() {
		return this.apr;
	}
	public void setMay(Double value) {
		this.may = value;
	}
	
	public Double getMay() {
		return this.may;
	}
	public void setJun(Double value) {
		this.jun = value;
	}
	
	public Double getJun() {
		return this.jun;
	}
	public void setJuly(Double value) {
		this.july = value;
	}
	
	public Double getJuly() {
		return this.july;
	}
	public void setAug(Double value) {
		this.aug = value;
	}
	
	public Double getAug() {
		return this.aug;
	}
	public void setSept(Double value) {
		this.sept = value;
	}
	
	public Double getSept() {
		return this.sept;
	}
	public void setOct(Double value) {
		this.oct = value;
	}
	
	public Double getOct() {
		return this.oct;
	}
	public void setNov(Double value) {
		this.nov = value;
	}
	
	public Double getNov() {
		return this.nov;
	}
	public void setDec(Double value) {
		this.dec = value;
	}
	
	public Double getDec() {
		return this.dec;
	}
	public void setYearly(Double value) {
		this.yearly = value;
	}
	
	public Double getYearly() {
		return this.yearly;
	}
	public void setFtype(java.lang.Integer value) {
		this.ftype = value;
	}
	
	public java.lang.Integer getFtype() {
		return this.ftype;
	}
	
	private StatisticItem statisticItem;
	
	public void setStatisticItem(StatisticItem statisticItem){
		this.statisticItem = statisticItem;
	}
	
	public StatisticItem getStatisticItem() {
		return statisticItem;
	}

	public String toString() {
		return new ToStringBuilder(this)
			.append("Statisticid",getStatisticid())
			.append("Itemid",getItemid())
			.append("Jan",getJan())
			.append("Feb",getFeb())
			.append("Mar",getMar())
			.append("Apr",getApr())
			.append("May",getMay())
			.append("Jun",getJun())
			.append("July",getJuly())
			.append("Aug",getAug())
			.append("Sept",getSept())
			.append("Oct",getOct())
			.append("Nov",getNov())
			.append("Dec",getDec())
			.append("Yearly",getYearly())
			.append("Ftype",getFtype())
			.toString();
	}
	
	public int hashCode() {
		return new HashCodeBuilder()
			.append(getStatisticid())
			.append(getItemid())
			.append(getJan())
			.append(getFeb())
			.append(getMar())
			.append(getApr())
			.append(getMay())
			.append(getJun())
			.append(getJuly())
			.append(getAug())
			.append(getSept())
			.append(getOct())
			.append(getNov())
			.append(getDec())
			.append(getYearly())
			.append(getFtype())
			.toHashCode();
	}
	
	public boolean equals(Object obj) {
		if(obj instanceof TaskStatistic == false) return false;
		if(this == obj) return true;
		TaskStatistic other = (TaskStatistic)obj;
		return new EqualsBuilder()
			.append(getStatisticid(),other.getStatisticid())
			.append(getItemid(),other.getItemid())
			.append(getJan(),other.getJan())
			.append(getFeb(),other.getFeb())
			.append(getMar(),other.getMar())
			.append(getApr(),other.getApr())
			.append(getMay(),other.getMay())
			.append(getJun(),other.getJun())
			.append(getJuly(),other.getJuly())
			.append(getAug(),other.getAug())
			.append(getSept(),other.getSept())
			.append(getOct(),other.getOct())
			.append(getNov(),other.getNov())
			.append(getDec(),other.getDec())
			.append(getYearly(),other.getYearly())
			.append(getFtype(),other.getFtype())
			.isEquals();
	}

	public String getItemname() {
		return itemname;
	}

	public void setItemname(String itemname) {
		this.itemname = itemname;
	}

	public List<StatisticItem> getItemnameList() {
		return itemnameList;
	}

	public void setItemnameList(List<StatisticItem> itemnameList) {
		this.itemnameList = itemnameList;
	}

	public Integer getFkey() {
		return fkey;
	}

	public void setFkey(Integer fkey) {
		this.fkey = fkey;
	}

	public java.lang.Long getFclassid() {
		return fclassid;
	}

	public void setFclassid(java.lang.Long fclassid) {
		this.fclassid = fclassid;
	}

	public List<StatisticClass> getStatisticclassList() {
		return statisticclassList;
	}

	public void setStatisticclassList(List<StatisticClass> statisticclassList) {
		this.statisticclassList = statisticclassList;
	}

}

