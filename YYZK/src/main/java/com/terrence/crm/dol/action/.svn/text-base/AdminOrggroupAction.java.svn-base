	}

	
	public void setAdminOrggroupQuery(AdminOrggroupQuery adminOrggroupQuery) {
		this.adminOrggroupQuery = adminOrggroupQuery;
	}

	public void setAdminOrggroupId(java.lang.Long adminOrggroupId) {
		this.adminOrggroupId = adminOrggroupId;
