package com.terrence.crm.dol.query;

import java.io.Serializable;

import org.apache.commons.lang.builder.ToStringBuilder;
import org.apache.commons.lang.builder.ToStringStyle;

import com.terrence.crm.dol.base.BaseQuery;
import com.terrence.crm.dol.bean.Device;
import com.terrence.crm.dol.bean.EmployeeTask;


public class DeviceWorkQuery extends BaseQuery implements Serializable {
	

	
	//columns START
	private java.lang.Long workid;
	private java.lang.Long deviceid;
	private java.lang.Long employeetaskid;
	private java.lang.String workdate;
	private java.lang.Long runtime;
	//columns END
	private java.lang.Long  procedurerid;
	private java.lang.String fbillno;
	private java.lang.Integer classtypeid;
	private java.lang.Long  icmoid;
	private java.lang.Long itemprocedureid;
	
	
	
	

		public java.lang.Long getItemprocedureid() {
		return itemprocedureid;
	}

	public void setItemprocedureid(java.lang.Long itemprocedureid) {
		this.itemprocedureid = itemprocedureid;
	}

		public java.lang.Long getProcedurerid() {
		return procedurerid;
	}

	public void setProcedurerid(java.lang.Long procedurerid) {
		this.procedurerid = procedurerid;
	}

	public java.lang.String getFbillno() {
		return fbillno;
	}

	public void setFbillno(java.lang.String fbillno) {
		this.fbillno = fbillno;
	}

	public java.lang.Integer getClasstypeid() {
		return classtypeid;
	}

	public void setClasstypeid(java.lang.Integer classtypeid) {
		this.classtypeid = classtypeid;
	}

	public java.lang.Long getIcmoid() {
		return icmoid;
	}

	public void setIcmoid(java.lang.Long icmoid) {
		this.icmoid = icmoid;
	}

		public void setWorkid(java.lang.Long value) {
			this.workid = value;
		}
		
		public java.lang.Long getWorkid() {
			return this.workid;
		}
		public void setDeviceid(java.lang.Long value) {
			this.deviceid = value;
		}
		
		public java.lang.Long getDeviceid() {
			return this.deviceid;
		}
		public void setEmployeetaskid(java.lang.Long value) {
			this.employeetaskid = value;
		}
		
		public java.lang.Long getEmployeetaskid() {
			return this.employeetaskid;
		}
		public void setWorkdate(java.lang.String value) {
			this.workdate = value;
		}
		
		public java.lang.String getWorkdate() {
			return this.workdate;
		}
		public void setRuntime(java.lang.Long value) {
			this.runtime = value;
		}
		
		public java.lang.Long getRuntime() {
			return this.runtime;
		}
	
	private Device device;
	
	public void setDevice(Device device){
		this.device = device;
	}
	
	public Device getDevice() {
		return device;
	}
	
	private EmployeeTask employeeTask;
	
	public void setEmployeeTask(EmployeeTask employeeTask){
		this.employeeTask = employeeTask;
	}
	
	public EmployeeTask getEmployeeTask() {
		return employeeTask;
	}

	public String toString() {
		return ToStringBuilder.reflectionToString(this,ToStringStyle.MULTI_LINE_STYLE);
	}
}

