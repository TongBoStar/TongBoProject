    <select id="topicitemlist.data.select.itemwait" resultMap="TopicItem.data.in.itemwait">
    	<!-- select t_sortcodesec.secname secname,t_sortcodesec.secid sortsecid,t_topic_item.testitemid,
		itemval.valid,itemval.waitid,itemval.topicid,itemval.fileno,itemval.filepath,itemval.testitemid,itemval.secid,
		itemval.comments,itemval.itemcode,itemval.ffilename,itemval.fresult from t_topic_item left join(
		select t_topic_item_val.* from t_topic_item_val ,t_item_wait
		where  t_item_wait.waitid=t_topic_item_val.waitid and t_item_wait.waitid=#waitid#) itemval
		on t_topic_item.testitemid=itemval.testitemid,t_sortcodesec 
		where t_topic_item.secid=t_sortcodesec.secid 
		and t_topic_item.topicid=#topicid# -->
		
		select t_sortcodesec.secname secname,t_sortcodesec.secid sortsecid,t_topic_item.testitemid,
		itemval.valid,itemval.waitid,itemval.topicid,itemval.fileno,itemval.filepath,itemval.testitemid,itemval.secid,
		itemval.comments,itemval.itemcode,itemval.ffilename,itemval.fresult from 
		( select t_topic_item_val.testitemid,t_topic_item_val.topicid,t_topic_item_val.secid from t_topic_item_val where t_topic_item_val.waitid=#waitid# union select t_topic_item.testitemid,t_topic_item.topicid,t_topic_item.secid  from t_topic_item,t_sortcodesec where t_topic_item.topicid=#topicid# and t_topic_item.secid=t_sortcodesec.secid) 
		 t_topic_item left join(
		select t_topic_item_val.* from t_topic_item_val ,t_item_wait
		where  t_item_wait.waitid=t_topic_item_val.waitid and t_item_wait.waitid=#waitid#) itemval
		on t_topic_item.testitemid=itemval.testitemid,t_sortcodesec 
		where t_topic_item.secid=t_sortcodesec.secid 
		and t_topic_item.topicid=#topicid#
		
    </select>
    
