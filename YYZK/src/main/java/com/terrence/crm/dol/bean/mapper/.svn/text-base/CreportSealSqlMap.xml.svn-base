        	dftunepacval ,
        	dftuneptimes ,
        	dftunepmedium ,
        	dfpdegree ,
        	fresult ,
        	fchecker ,
        	fcheckername ,
        	examiner ,
        	examinername ,
        	createtime ,
        	lastedittime ,
        	lastediter ,
        	setpressure,
        	auditorId ,
        	auditorName ,
        	authorId ,
        	authorName ,
        	fstatus,
        	designpressUnit,
        	setpressureUnit
        ) VALUES (
        	#reportid# ,
        	#prirecord# ,
        	#checkcraft# ,
        	#designpress# ,
        	#standard1# ,
        	#standard2# ,
        	#standard3# ,
        	#checkpress# ,
        	#inpreval# ,
        	#inpacval# ,
