	public boolean equals(Object obj) {
		if(obj instanceof FactoryReport == false) return false;
		if(this == obj) return true;
		FactoryReport other = (FactoryReport)obj;
		return new EqualsBuilder()
			.append(getReportid(),other.getReportid())
			.append(getItemcode(),other.getItemcode())
			.append(getItemname(),other.getItemname())
			.append(getItemmode(),other.getItemmode())
			.append(getSerial(),other.getSerial())
			.append(getFname(),other.getFname())
			.append(getFpath(),other.getFpath())
			.append(getCreatedate(),other.getCreatedate())
			.append(getReportno(),other.getReportno())
			.append(getReportdate(),other.getReportdate())
			.append(getOrderno(),other.getOrderno())
			.append(getCustomername(),other.getCustomername())
			.append(getFmodel1(),other.getFmodel1())
			.append(getFmodel2(),other.getFmodel2())
			.append(getFmodel3(),other.getFmodel3())
			.append(getFserial1(),other.getFserial1())
			.append(getFserial2(),other.getFserial2())
			.append(getFserial3(),other.getFserial3())
			.append(getBillno(),other.getBillno())
			.append(getPdtype(),other.getPdtype())
			.append(getEmpid(),other.getEmpid())
			.append(getQc(),other.getQc())
			.append(getQcdate(),other.getQcdate())
			.append(getViewemp(),other.getViewemp())
			.append(getViewdate(),other.getViewdate())
			.isEquals();
	}

