			.append(getFtrack())
			.append(getForderrector())
			.append(getFproductprincipal())
			.toHashCode();
	}
	
	public boolean equals(Object obj) {
		if(obj instanceof Item == false) return false;
		if(this == obj) return true;
		Item other = (Item)obj;
		return new EqualsBuilder()
			.append(getFitemid(),other.getFitemid())
			.append(getFnumber(),other.getFnumber())
			.append(getFname(),other.getFname())
			.append(getFmodel(),other.getFmodel())
			.append(getF106(),other.getF106())
			.append(getF105(),other.getF105())
