		if(this == obj) return true;
		RpExchange other = (RpExchange)obj;
		return new EqualsBuilder()
			.append(getFrpexid(),other.getFrpexid())
			.append(getRpid(),other.getRpid())
			.append(getFbillid(),other.getFbillid())
			.append(getFentryid(),other.getFentryid())
