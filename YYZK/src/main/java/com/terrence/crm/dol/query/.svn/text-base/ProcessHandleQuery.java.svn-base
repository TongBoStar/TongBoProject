package com.terrence.crm.dol.query;
import com.terrence.crm.dol.base.BaseQuery;
import com.terrence.crm.dol.utils.DateConvertUtils;

public class ProcessHandleQuery extends BaseQuery implements java.io.Serializable {
	/**
	 * 序列化ID
	 */
	private static final long serialVersionUID = 1L;
	/**
	 * 唯一标识
	 */
	private Long id;
	/**
	 * 节点Id
	 */
	private Long nodeId;
	/**
	 * 处理人
	 */
	private Long handler;
	/**
	 * 处理部门
	 */
	private Long dispatchdep;

	public Long getId(){
		return this.id;
	}
	public void setId(Long id){
		this.id=id;
	}
	public Long getNodeId(){
		return this.nodeId;
	}
	public void setNodeId(Long nodeId){
		this.nodeId=nodeId;
	}
	public Long getHandler(){
		return this.handler;
	}
	public void setHandler(Long handler){
		this.handler=handler;
	}
	public Long getDispatchdep(){
		return this.dispatchdep;
	}
	public void setDispatchdep(Long dispatchdep){
		this.dispatchdep=dispatchdep;
	}

}
