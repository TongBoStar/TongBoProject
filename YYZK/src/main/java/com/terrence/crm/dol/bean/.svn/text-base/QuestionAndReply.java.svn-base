package com.terrence.crm.dol.bean;

import java.io.Serializable;

import com.terrence.crm.dol.base.BaseEntity;

/**
 * 问题和回复类
 * @author wangtongbo 
 * E-mail:tongbo.wang@terrence.com.cn
 * @version 创建时间：2014-7-17 下午6:11:33
 */
public class QuestionAndReply extends BaseEntity implements Serializable{

	/**
	 * 
	 */
	private static final long serialVersionUID = -8687976785803485418L;
	/**
	 * ID
	 */
	private Long questionAndReplyId;
	/**
	 * 审核单ID
	 */
	private Long contractReviewId;
	/**
	 * 订单编号
	 */
	private String orderCode;
	/**
	 * 问题
	 */
	private String question;
	/**
	 * 提问人
	 */
	private String questioner;
	/**
	 * 提问时间
	 */
	private String questionTime;
	/**
	 * 回复
	 */
	private String reply;
	/**
	 * 回复人
	 */
	private String replyer;
	/**
	 * 回复时间
	 */
	private String replyTime;
	/**
	 * 状态 是否同意 0不同意 1同意
	 */
	private Integer fstatus;
	
	public Long getQuestionAndReplyId() {
		return questionAndReplyId;
	}
	public void setQuestionAndReplyId(Long questionAndReplyId) {
		this.questionAndReplyId = questionAndReplyId;
	}
	public String getOrderCode() {
		return orderCode;
	}
	public void setOrderCode(String orderCode) {
		this.orderCode = orderCode;
	}
	public String getQuestion() {
		return question;
	}
	public void setQuestion(String question) {
		this.question = question;
	}
	public String getQuestioner() {
		return questioner;
	}
	public void setQuestioner(String questioner) {
		this.questioner = questioner;
	}
	public String getQuestionTime() {
		return questionTime;
	}
	public void setQuestionTime(String questionTime) {
		this.questionTime = questionTime;
	}
	public String getReply() {
		return reply;
	}
	public void setReply(String reply) {
		this.reply = reply;
	}
	public String getReplyer() {
		return replyer;
	}
	public void setReplyer(String replyer) {
		this.replyer = replyer;
	}
	public String getReplyTime() {
		return replyTime;
	}
	public void setReplyTime(String replyTime) {
		this.replyTime = replyTime;
	}
	public Long getContractReviewId() {
		return contractReviewId;
	}
	public void setContractReviewId(Long contractReviewId) {
		this.contractReviewId = contractReviewId;
	}
	public Integer getFstatus() {
		return fstatus;
	}
	public void setFstatus(Integer fstatus) {
		this.fstatus = fstatus;
	}
}
