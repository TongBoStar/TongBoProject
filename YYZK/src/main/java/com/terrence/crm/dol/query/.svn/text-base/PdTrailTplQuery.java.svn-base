package com.terrence.crm.dol.query;

import java.io.Serializable;

import org.apache.commons.lang.builder.ToStringBuilder;
import org.apache.commons.lang.builder.ToStringStyle;

import com.terrence.crm.dol.base.BaseQuery;


public class PdTrailTplQuery extends BaseQuery implements Serializable {
	

	
	//columns START
	private java.lang.Long rid;
	private java.lang.Long tplid;
	private java.lang.String itemcode;
	//columns END
	private java.lang.Long trialid;
		
	
	public java.lang.Long getTrialid() {
		return trialid;
	}

	public void setTrialid(java.lang.Long trialid) {
		this.trialid = trialid;
	}

	private Integer isdefault;

	public Integer getIsdefault() {
		return isdefault;
	}

	public void setIsdefault(Integer isdefault) {
		this.isdefault = isdefault;
	}



		public void setRid(java.lang.Long value) {
			this.rid = value;
		}
		
		public java.lang.Long getRid() {
			return this.rid;
		}
		public void setTplid(java.lang.Long value) {
			this.tplid = value;
		}
		
		public java.lang.Long getTplid() {
			return this.tplid;
		}
		public void setItemcode(java.lang.String value) {
			this.itemcode = value;
		}
		
		public java.lang.String getItemcode() {
			return this.itemcode;
		}

	public String toString() {
		return ToStringBuilder.reflectionToString(this,ToStringStyle.MULTI_LINE_STYLE);
	}
}

