package com.terrence.crm.dol.bean;

import javax.persistence.Entity;

import org.apache.commons.lang.builder.EqualsBuilder;
import org.apache.commons.lang.builder.HashCodeBuilder;
import org.apache.commons.lang.builder.ToStringBuilder;

import com.terrence.crm.dol.base.BaseEntity;
import com.terrence.crm.dol.utils.DateConvertUtils;

@Entity
public class ProItem extends BaseEntity implements java.io.Serializable {
	
	
	//columns START
	private java.lang.Long aid;
	private java.lang.Long fitemid;
	private java.lang.String itemcode;
	private java.lang.String itemname;
	private java.lang.String fmode;
	private java.lang.String fdescript;
	private java.lang.String flevel;
	private java.lang.String fmaker;
	private java.lang.String fshortname;
	private java.lang.String srcarea;
	private Double buyamount;
	private Double avgamount;
	private Double rpamount;
	private Double costamount;
	private Double listamount;
	//columns END


	public void setAid(java.lang.Long value) {
		this.aid = value;
	}
	
	public java.lang.Long getAid() {
		return this.aid;
	}
	public void setFitemid(java.lang.Long value) {
		this.fitemid = value;
	}
	
	public java.lang.Long getFitemid() {
		return this.fitemid;
	}
	public void setItemcode(java.lang.String value) {
		this.itemcode = value;
	}
	
	public java.lang.String getItemcode() {
		return this.itemcode;
	}
	public void setItemname(java.lang.String value) {
		this.itemname = value;
	}
	
	public java.lang.String getItemname() {
		return this.itemname;
	}
	public void setFmode(java.lang.String value) {
		this.fmode = value;
	}
	
	public java.lang.String getFmode() {
		return this.fmode;
	}
	public void setFdescript(java.lang.String value) {
		this.fdescript = value;
	}
	
	public java.lang.String getFdescript() {
		return this.fdescript;
	}
	public void setFlevel(java.lang.String value) {
		this.flevel = value;
	}
	
	public java.lang.String getFlevel() {
		return this.flevel;
	}
	public void setFmaker(java.lang.String value) {
		this.fmaker = value;
	}
	
	public java.lang.String getFmaker() {
		return this.fmaker;
	}
	public void setFshortname(java.lang.String value) {
		this.fshortname = value;
	}
	
	public java.lang.String getFshortname() {
		return this.fshortname;
	}
	public void setSrcarea(java.lang.String value) {
		this.srcarea = value;
	}
	
	public java.lang.String getSrcarea() {
		return this.srcarea;
	}
	public void setBuyamount(Double value) {
		this.buyamount = value;
	}
	
	public Double getBuyamount() {
		return this.buyamount;
	}
	public void setAvgamount(Double value) {
		this.avgamount = value;
	}
	
	public Double getAvgamount() {
		return this.avgamount;
	}
	public void setRpamount(Double value) {
		this.rpamount = value;
	}
	
	public Double getRpamount() {
		return this.rpamount;
	}
	public void setCostamount(Double value) {
		this.costamount = value;
	}
	
	public Double getCostamount() {
		return this.costamount;
	}
	public void setListamount(Double value) {
		this.listamount = value;
	}
	
	public Double getListamount() {
		return this.listamount;
	}

	public String toString() {
		return new ToStringBuilder(this)
			.append("Aid",getAid())
			.append("Fitemid",getFitemid())
			.append("Itemcode",getItemcode())
			.append("Itemname",getItemname())
			.append("Fmode",getFmode())
			.append("Fdescript",getFdescript())
			.append("Flevel",getFlevel())
			.append("Fmaker",getFmaker())
			.append("Fshortname",getFshortname())
			.append("Srcarea",getSrcarea())
			.append("Buyamount",getBuyamount())
			.append("Avgamount",getAvgamount())
			.append("Rpamount",getRpamount())
			.append("Costamount",getCostamount())
			.append("Listamount",getListamount())
			.toString();
	}
	
	public int hashCode() {
		return new HashCodeBuilder()
			.append(getAid())
			.append(getFitemid())
			.append(getItemcode())
			.append(getItemname())
			.append(getFmode())
			.append(getFdescript())
			.append(getFlevel())
			.append(getFmaker())
			.append(getFshortname())
			.append(getSrcarea())
			.append(getBuyamount())
			.append(getAvgamount())
			.append(getRpamount())
			.append(getCostamount())
			.append(getListamount())
			.toHashCode();
	}
	
	public boolean equals(Object obj) {
		if(obj instanceof ProItem == false) return false;
		if(this == obj) return true;
		ProItem other = (ProItem)obj;
		return new EqualsBuilder()
			.append(getAid(),other.getAid())
			.append(getFitemid(),other.getFitemid())
			.append(getItemcode(),other.getItemcode())
			.append(getItemname(),other.getItemname())
			.append(getFmode(),other.getFmode())
			.append(getFdescript(),other.getFdescript())
			.append(getFlevel(),other.getFlevel())
			.append(getFmaker(),other.getFmaker())
			.append(getFshortname(),other.getFshortname())
			.append(getSrcarea(),other.getSrcarea())
			.append(getBuyamount(),other.getBuyamount())
			.append(getAvgamount(),other.getAvgamount())
			.append(getRpamount(),other.getRpamount())
			.append(getCostamount(),other.getCostamount())
			.append(getListamount(),other.getListamount())
			.isEquals();
	}
}

