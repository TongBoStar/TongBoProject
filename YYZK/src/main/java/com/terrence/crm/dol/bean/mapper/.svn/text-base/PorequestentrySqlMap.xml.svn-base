  	<!-- 用于采购计划详细中已购清单查询 -->
    <select id="Porequestentry.Itemcode.Sum" resultMap="RM.Porequestentry.Itemcode.Sum">
select t.sourceCode,t.itemcode,isnull(t1.qty,0) qty1,isnull(t2.qty,0) qty2,isnull(t3.qty,0) qty3,isnull(t4.qty,0) qty4,
isnull(t5.qty,0) qty5,isnull(t6.qty,0) qty6,1 as finterid from 
(
select distinct alltable.sourceCode,alltable.itemcode
from (
select t_porequest.sourceCode,t_porequestentry.itemcode from t_porequest left join t_porequestentry on t_porequest.finterid = t_porequestentry.finterid
where t_porequest.fstatus in (0,2) and t_porequest.sourceCode is not null and t_porequestentry.itemcode is not null and t_porequestentry.amount is not null
union all
select t_porequest.sourceCode,t_porequestentry.itemcode from t_porequest left join t_porequestentry on t_porequest.finterid = t_porequestentry.finterid
where t_porequest.fstatus=1 and t_porequest.sourceCode is not null   and t_porequestentry.itemcode is not null
	AND t_porequestentry.fmrpclosed  = 0 
    AND t_porequestentry.ispush = 0 
    AND t_porequest.fclosed = 0 
union all
select t_porequest.sourceCode,t_poorderentry.itemcode from t_poorder left join t_poorderentry on t_poorder.finterid = t_poorderentry.finterid
left join t_porequest on t_poorderentry.fsourcebillno=t_porequest.fbillno
where t_poorder.fstatus in (0,2) and t_poorderentry.fsourcebillno like '%POREQ%' and t_poorderentry.itemcode is not null
and t_porequest.sourceCode is not null
union all
select t_porequest.sourceCode,t_poorderentry.itemcode from t_poorder left join t_poorderentry on t_poorder.finterid = t_poorderentry.finterid
left join t_porequest on t_poorderentry.fsourcebillno=t_porequest.fbillno
where t_poorder.fstatus =1   AND t_poorder.fclosed = 0 
and t_poorderentry.fsourcebillno like '%POREQ%' and t_poorderentry.itemcode is not null  AND t_poorderentry.mrpclosed = 0 
and t_porequest.sourceCode is not null
) alltable 
) t
left join 
(
select t_porequest.sourceCode,t_porequestentry.itemcode,sum(t_porequestentry.amount) qty from t_porequest left join t_porequestentry on t_porequest.finterid = t_porequestentry.finterid
 where t_porequest.fstatus=0 and t_porequest.sourceCode is not null  and t_porequestentry.itemcode is not null
 group by t_porequest.sourceCode,t_porequestentry.itemcode
) t1 on t.sourceCode = t1.sourceCode and t.itemcode = t1.itemcode
left join 
(
 select t_porequest.sourceCode,t_porequestentry.itemcode,sum(t_porequestentry.amount) qty from t_porequest left join t_porequestentry on t_porequest.finterid = t_porequestentry.finterid
 where t_porequest.fstatus=2 and t_porequest.sourceCode is not null  and t_porequestentry.itemcode is not null
 group by t_porequest.sourceCode,t_porequestentry.itemcode
) t2 on t.sourceCode = t2.sourceCode and t.itemcode = t2.itemcode
left join 
(
 select t_porequest.sourceCode,t_porequestentry.itemcode,sum(t_porequestentry.amount) qty from t_porequest left join t_porequestentry on t_porequest.finterid = t_porequestentry.finterid
 where t_porequest.fstatus=1 and t_porequest.sourceCode is not null   and t_porequestentry.itemcode is not null
		AND t_porequestentry.fmrpclosed  = 0 
        AND t_porequestentry.ispush = 0 
        AND t_porequest.fclosed = 0 
 group by t_porequest.sourceCode,t_porequestentry.itemcode
) t3 on t.sourceCode = t3.sourceCode and t.itemcode = t3.itemcode
left join 
(
 select t_porequest.sourceCode,t_poorderentry.itemcode,sum(t_poorderentry.famount) qty from t_poorder left join t_poorderentry on t_poorder.finterid = t_poorderentry.finterid
left join t_porequest on t_poorderentry.fsourcebillno=t_porequest.fbillno
where t_poorder.fstatus =0 and t_poorderentry.fsourcebillno like '%POREQ%' and t_poorderentry.itemcode is not null
and t_porequest.sourceCode is not null
group by t_porequest.sourceCode,t_poorderentry.itemcode
) t4 on t.sourceCode = t4.sourceCode and t.itemcode = t4.itemcode
left join 
(
select t_porequest.sourceCode,t_poorderentry.itemcode,sum(t_poorderentry.famount) qty from t_poorder left join t_poorderentry on t_poorder.finterid = t_poorderentry.finterid
left join t_porequest on t_poorderentry.fsourcebillno=t_porequest.fbillno
where t_poorder.fstatus =2 and t_poorderentry.fsourcebillno like '%POREQ%' and t_poorderentry.itemcode is not null
and t_porequest.sourceCode is not null
group by t_porequest.sourceCode,t_poorderentry.itemcode
) t5 on t.sourceCode = t5.sourceCode and t.itemcode = t5.itemcode
left join 
(
select t_porequest.sourceCode,t_poorderentry.itemcode,sum(t_poorderentry.famount) qty from t_poorder left join t_poorderentry on t_poorder.finterid = t_poorderentry.finterid
left join t_porequest on t_poorderentry.fsourcebillno=t_porequest.fbillno
where t_poorder.fstatus =1   AND t_poorder.fclosed = 0 
and t_poorderentry.fsourcebillno like '%POREQ%' and t_poorderentry.itemcode is not null  AND t_poorderentry.mrpclosed = 0 
and t_porequest.sourceCode is not null
group by t_porequest.sourceCode,t_poorderentry.itemcode
) t6 on t.sourceCode = t6.sourceCode and t.itemcode = t6.itemcode
where t.sourceCode = #sourcecode#
order by t.sourceCode,t.itemcode
    </select>
</sqlMap>
