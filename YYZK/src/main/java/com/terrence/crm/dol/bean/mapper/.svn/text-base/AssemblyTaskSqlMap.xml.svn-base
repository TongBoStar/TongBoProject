      select tob.parentcode,isnull(tib.fqtyall,0)-isnull(tob.parentqty,0)*#demand#-isnull(tbi.existdemand,0) amount
	  from t_order_bom tob 
	  left join (select sum(isnull(fqty,0))+SUM(ISNULL(fqty2,0)) fqtyall,itemcode from t_inventory_batch group by itemcode) tib on tob.parentcode = tib.itemcode 
	  left join t_base_item tbi on tob.parentcode = tbi.itemcode 
	  where tob.itemcode=#itemcode#
    </select>
    <select id="AssemblyTask.enoughDetail" resultMap="RM.AssemblyTask.EnoughDetail">
      select  tb.itemcode,tb.parentcode model,tb.parentname remarks,isnull(tb.parentqty,0) workload,
      isnull(tat.demand,0) demand,isnull(tib.existdemand,0) demanded,isnull(tib.fqtyall,0) putfqty
	  from t_bom tb 
	  left join (select sum(isnull(fqty,0))+SUM(ISNULL(fqty2,0)) fqtyall,SUM(ISNULL(fqtylock,0)) fqtylock,itemcode from t_inventory_batch group by itemcode) tib on tb.parentcode = tib.itemcode 
	  left join t_base_item tbi on tb.parentcode = tbi.itemcode 
	  left join t_assembly_task tat on tb.itemcode = tat.itemcode and tat.id=#id#
	  where tb.itemcode=#itemcode#
    </select>
