@Scope("prototype")
@Namespace("/baseitemApply")
@Results( { @Result(name = CrudActionSupport.RELOAD, location = "baseitemApply."+ CrudActionSupport.ACTIONSUFFIX
		+ "?page.pageRequest=${page.pageRequest}", type = "redirect") })
public class BaseitemApplyAction extends CrudActionSupport<BaseitemApply> implements ApplicationContextAware {

	@Autowired
	@Qualifier("baseitemApplyService")
	private IBaseitemApplyService baseitemApplyService;

	private BaseitemApply entity;
	private ProcessExetask processExetask;
	private BaseitemApplyQuery baseitemApplyQuery;

	private java.lang.Long baseitemApplyId;

	/** 分页器 */
	private Page<BaseitemApply> page = new Page<BaseitemApply>();

	/** Json操作信息 */
	private String[] resultInfo = new String[3];

	/** 所选择的编号列表 */
	private List<java.lang.Long> checkedIdList;

	private ApplicationContext ctx;
	private List<BaseitemApply> baseitemApplyList;

	/*
	 * (non-Javadoc)
	 * @see com.disney.dol.base.CrudActionSupport#prepareModel()
	 */
	@Override
	protected void prepareModel() throws Exception {
		if (baseitemApplyId != null) {
			entity = baseitemApplyService.getById(baseitemApplyId);
		} else {
			entity = new BaseitemApply();
		}
	}

	/*
	 * (non-Javadoc)
	 * @see com.disney.dol.base.CrudActionSupport#delete()
	 */
	@Override
	public String delete() throws Exception {
		try {
			if(null!=checkedIdList && checkedIdList.size()>0){
				baseitemApplyService.deleteByMultipleId(checkedIdList);
			}else{
				baseitemApplyService.deleteById(baseitemApplyId);
			}
			resultInfo[0] = "0";
			resultInfo[1] = "删除成功";
			addActionMessage("删除成功");
		} catch (Exception e) {
			logger.error(e.getMessage());
			resultInfo[0] = "-1";
			resultInfo[1] = "删除失败";
			addActionMessage("删除失败");
		}
		return RELOAD;
	}

	/*
	 * (non-Javadoc)
	 * @see com.disney.dol.base.CrudActionSupport#deleteToJson()
	 */
	public String deleteToJson() throws Exception {
		try {
			delete();
		} catch (Exception e) {
			logger.error(e.getMessage());
			resultInfo[0] = "-1";
			resultInfo[1] = "删除失败";
		}
		return JSON;
	}

	/*
	 * (non-Javadoc)
	 * @see com.disney.dol.base.CrudActionSupport#input()
	 */
	@Override
	public String input() throws Exception {
		return INPUT;
	}

	public String listToQuery() throws Exception {
		if (baseitemApplyQuery == null) {
			baseitemApplyQuery = new BaseitemApplyQuery();
		}
		try {
			baseitemApplyList = baseitemApplyService.findList(baseitemApplyQuery);
			resultInfo[0] = "0";
			resultInfo[1] = "查询列表成功";
		} catch (ServiceException e) {
			logger.error(e.getMessage());
			resultInfo[0] = "-1";
			resultInfo[1] = e.getMessage();
		} catch (Exception e) {
			logger.error(e.getMessage());
			resultInfo[0] = "-1";
			resultInfo[1] = "查询列表失败";
		}
		return JSON;
	}

	/*
	 * (non-Javadoc)
	 * @see com.disney.dol.base.CrudActionSupport#list()
	 */
	@Override
	public String list() throws Exception {
		if (!page.isOrderBySetted()) {
			page.setOrderBy("id");
			page.setOrder(Page.DESC);
		}
		if (baseitemApplyQuery == null) {
			baseitemApplyQuery = new BaseitemApplyQuery();
		}
		try {
			page = baseitemApplyService.findPage(page, baseitemApplyQuery);
			resultInfo[0] = "0";
			resultInfo[1] = "查询列表成功";
		} catch (ServiceException e) {
			logger.error(e.getMessage());
			resultInfo[0] = "-1";
			resultInfo[1] = e.getMessage();
		} catch (Exception e) {
			logger.error(e.getMessage());
			resultInfo[0] = "-1";
			resultInfo[1] = "查询列表失败";
		}
		return SUCCESS;
	}

    /**
     * 查询列表
     * @return
     * @throws Exception
     */
	public String listQuery() throws Exception {
		if (!page.isOrderBySetted()) {
			page.setOrderBy("id");
			page.setOrder(Page.DESC);
		}
		if (baseitemApplyQuery == null) {
			baseitemApplyQuery = new BaseitemApplyQuery();
