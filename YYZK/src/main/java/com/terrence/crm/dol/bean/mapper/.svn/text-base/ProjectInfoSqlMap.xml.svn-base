<select id="ProjectInfo.findPage" resultMap="RM.ProjectInfoList">
	select <include refid="ProjectInfo.columns"/>, s1.structureName as pressurestructurename,s2.structureName as meterstructurename  FROM t_project_info
	left join t_structure s1 on s1.id=t_project_info.pressureStructure 
	left join t_structure s2 on s2.id=t_project_info.meterStructure 
 	where 1=1
	<isNotEmpty property="notTop">  
		and id not in(
			select $notTop$ id from t_project_info <include refid="ProjectInfo.findPage.where"/>
			<isNotEmpty property="sortColumns"> 
