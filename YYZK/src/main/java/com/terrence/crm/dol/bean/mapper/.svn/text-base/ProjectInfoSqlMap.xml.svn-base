<select id="ProjectInfo.getById" resultMap="RM.ProjectInfoList">
	select <include refid="ProjectInfo.columns"/> , s1.structureName as pressurestructurename,s2.structureName as meterstructurename  
	FROM t_project_info
	left join t_structure s1 on s1.id=t_project_info.pressureStructure 
	left join t_structure s2 on s2.id=t_project_info.meterStructure  
	where t_project_info.id=#id#  
</select>

<select id="ProjectInfo.getByProperty" resultMap="RM.ProjectInfo">
	select <include refid="ProjectInfo.columns"/> from t_project_info where $propertyName$=#propertyValue#
</select>

<delete id="ProjectInfo.delete">
	<![CDATA[
		DELETE FROM t_project_info WHERE id=#id#
	]]>
</delete>
<delete id="ProjectInfo.deleteMultiple">
	<![CDATA[
		DELETE FROM t_project_info WHERE id in ($id$)
	]]>
</delete>

