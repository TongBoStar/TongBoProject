	public String toString() {
		return new ToStringBuilder(this).append("Id", getId())
				.append("StdBatchQty", getStdBatchQty())
				.append("CostBomId", getCostBomId())
				.append("Cbrouting", getCbrouting())
				.append("StandardCost", getStandardCost())
				.append("StandardManHour", getStandardManHour())
				.append("StdPayRate", getStdPayRate())
				.append("ChgFeeRate", getChgFeeRate())
				.append("StdFixFeeRate", getStdFixFeeRate())
				.append("OutMachFee", getOutMachFee())
				.append("PieceRate", getPieceRate())
				.append("PovacctId", getPovacctId())
				.append("PivacctId", getPivacctId())
				.append("McvacctId", getMcvacctId())
				.append("PcvacctId", getPcvacctId())
				.append("SlacctId", getSlacctId())
				.append("CavacctId", getCavacctId())
				.append("CbappendRate", getCbappendRate())
				.append("CbappendProject", getCbappendProject())
				.append("OutMachFeeProject", getOutMachFeeProject())
				.append("InterId", getInterId()).toString();
	}

	public int hashCode() {
		return new HashCodeBuilder().append(getId()).append(getStdBatchQty())
				.append(getCostBomId()).append(getCbrouting())
				.append(getStandardCost()).append(getStandardManHour())
				.append(getStdPayRate()).append(getChgFeeRate())
				.append(getStdFixFeeRate()).append(getOutMachFee())
				.append(getPieceRate()).append(getPovacctId())
				.append(getPivacctId()).append(getMcvacctId())
				.append(getPcvacctId()).append(getSlacctId())
				.append(getCavacctId()).append(getCbappendRate())
				.append(getCbappendProject()).append(getOutMachFeeProject())
				.append(getInterId()).toHashCode();
	}

	public boolean equals(Object obj) {
		if (obj instanceof WebItemStandard == false)
			return false;
		if (this == obj)
			return true;
		WebItemStandard other = (WebItemStandard) obj;
		return new EqualsBuilder().append(getId(), other.getId())
				.append(getStdBatchQty(), other.getStdBatchQty())
				.append(getCostBomId(), other.getCostBomId())
				.append(getCbrouting(), other.getCbrouting())
				.append(getStandardCost(), other.getStandardCost())
				.append(getStandardManHour(), other.getStandardManHour())
				.append(getStdPayRate(), other.getStdPayRate())
				.append(getChgFeeRate(), other.getChgFeeRate())
				.append(getStdFixFeeRate(), other.getStdFixFeeRate())
				.append(getOutMachFee(), other.getOutMachFee())
				.append(getPieceRate(), other.getPieceRate())
				.append(getPovacctId(), other.getPovacctId())
				.append(getPivacctId(), other.getPivacctId())
				.append(getMcvacctId(), other.getMcvacctId())
				.append(getPcvacctId(), other.getPcvacctId())
				.append(getSlacctId(), other.getSlacctId())
				.append(getCavacctId(), other.getCavacctId())
				.append(getCbappendRate(), other.getCbappendRate())
				.append(getCbappendProject(), other.getCbappendProject())
				.append(getOutMachFeeProject(), other.getOutMachFeeProject())
				.append(getInterId(), other.getInterId()).isEquals();
	}
