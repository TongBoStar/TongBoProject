package com.terrence.crm.dol.query;

import java.io.Serializable;

import org.apache.commons.lang.builder.ToStringBuilder;
import org.apache.commons.lang.builder.ToStringStyle;

import com.terrence.crm.dol.base.BaseQuery;


public class ShortageQuery extends BaseQuery implements Serializable {
	

	
	//columns START
	private java.lang.Long shortageid;
	private java.lang.String itemcode;
	private java.lang.String itemname;
	private java.lang.Integer itemproperty;
	private java.lang.String itemmode;
	private java.lang.String ordercode;
	private java.util.Date deliverdateBegin;
	private java.util.Date deliverdateEnd;
	private Long demand;
	private Long mindemand;
	private java.lang.Integer status;
	private java.lang.Long orderid;
	private java.lang.Long productid;
	//columns END


		public void setShortageid(java.lang.Long value) {
			this.shortageid = value;
		}
		
		public java.lang.Long getShortageid() {
			return this.shortageid;
		}
		public void setItemcode(java.lang.String value) {
			this.itemcode = value;
		}
		
		public java.lang.String getItemcode() {
			return this.itemcode;
		}
		public void setItemname(java.lang.String value) {
			this.itemname = value;
		}
		
		public java.lang.String getItemname() {
			return this.itemname;
		}
		public void setItemproperty(java.lang.Integer value) {
			this.itemproperty = value;
		}
		
		public java.lang.Integer getItemproperty() {
			return this.itemproperty;
		}
		public void setItemmode(java.lang.String value) {
			this.itemmode = value;
		}
		
		public java.lang.String getItemmode() {
			return this.itemmode;
		}
		public void setOrdercode(java.lang.String value) {
			this.ordercode = value;
		}
		
		public java.lang.String getOrdercode() {
			return this.ordercode;
		}
	
		public void setDeliverdateBegin(java.util.Date value) {
			this.deliverdateBegin = value;
		}
		
		public java.util.Date getDeliverdateBegin() {
			return this.deliverdateBegin;
		}
		public void setDeliverdateEnd(java.util.Date value) {
			this.deliverdateEnd = value;
		}
		
		public java.util.Date getDeliverdateEnd() {
			return this.deliverdateEnd;
		}
	
		public void setDemand(Long value) {
			this.demand = value;
		}
		
		public Long getDemand() {
			return this.demand;
		}
		public void setMindemand(Long value) {
			this.mindemand = value;
		}
		
		public Long getMindemand() {
			return this.mindemand;
		}
		public void setStatus(java.lang.Integer value) {
			this.status = value;
		}
		
		public java.lang.Integer getStatus() {
			return this.status;
		}
		public void setOrderid(java.lang.Long value) {
			this.orderid = value;
		}
		
		public java.lang.Long getOrderid() {
			return this.orderid;
		}

	public String toString() {
		return ToStringBuilder.reflectionToString(this,ToStringStyle.MULTI_LINE_STYLE);
	}

	public java.lang.Long getProductid() {
		return productid;
	}

	public void setProductid(java.lang.Long productid) {
		this.productid = productid;
	}
}

