package com.terrence.core.dol.bean;

public class TableField {

	private Long id	;
	private String tableName;		//表名和table_info表关联
	private String name	;			//字段名
	private String dataType;		//字段类型  int string datetime
	private String length;			//字段长度
	private Integer isPrimaryKey;		//是否主键
	private String description;		//描述
	private Integer sort;				//顺序
	private String title;			//在webform中显示的名称
	private Integer isForm;			//是否在表单中显示
	private Integer isList;			//是否在列表中显示
	private Integer isBean;			//是否在bean类中中显示
	private String validform;		//表单验证规则
	private String inputType;		//表单输入类型 input textarea
	public Long getId() {
		return id;
	}
	public void setId(Long id) {
		this.id = id;
	}
	public String getTableName() {
		return tableName;
	}
	public void setTableName(String tableName) {
		this.tableName = tableName;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getDataType() {
		return dataType;
	}
	public void setDataType(String dataType) {
		this.dataType = dataType;
	}
	public String getLength() {
		return length;
	}
	public void setLength(String length) {
		this.length = length;
	}
	public Integer getIsPrimaryKey() {
		return isPrimaryKey;
	}
	public void setIsPrimaryKey(Integer isPrimaryKey) {
		this.isPrimaryKey = isPrimaryKey;
	}
	public String getDescription() {
		return description;
	}
	public void setDescription(String description) {
		this.description = description;
	}
	public Integer getSort() {
		return sort;
	}
	public void setSort(Integer sort) {
		this.sort = sort;
	}
	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	public Integer getIsForm() {
		return isForm;
	}
	public void setIsForm(Integer isForm) {
		this.isForm = isForm;
	}
	public Integer getIsList() {
		return isList;
	}
	public void setIsList(Integer isList) {
		this.isList = isList;
	}
	public String getValidform() {
		return validform;
	}
	public void setValidform(String validform) {
		this.validform = validform;
	}
	public String getInputType() {
		return inputType;
	}
	public void setInputType(String inputType) {
		this.inputType = inputType;
	}
	public Integer getIsBean() {
		return isBean;
	}
	public void setIsBean(Integer isBean) {
		this.isBean = isBean;
	}
}