}

function qychangeCode(){
			var sel = $("#qyselectCode").val();
			var txt = $('#qyselectCode').find('option:[value='+sel+']').text();
			$("#amanagerid").attr("value",sel);
			$("#amanagername").attr("value",txt);
			$("#qyDiv").css("display","none");
}

function qyenterCheck(){
	qysearch();
