		data :'dutyQuery.teamid='+teamid,
		success : function(data) {
		data = $.parseJSON(data);
				$.each(data.dutyList, function(i, o){
				dutyhtml+='<option value="'+o.dutyid+'">'+o.dutyname+'</option>';
